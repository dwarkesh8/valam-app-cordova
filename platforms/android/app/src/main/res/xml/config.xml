<?xml version='1.0' encoding='utf-8'?>
<widget id="com.charotarvalam.app" version="1.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <name>CharotarValamApp</name>
    <description>
        A sample demo android app for "Charotar Valam Brahmin"
    </description>
    <author email="contact@charotaritsolutions.com" href="https://charotaritsolutions.com">
        Charotar IT Solutions Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon density="mdpi" height="57" platform="android" src="www/img/app-logo.png" width="57" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
